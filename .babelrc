{
    "plugins": [
        "transform-vue-jsx",
        "transform-decorators-legacy",
        "add-module-exports",
        "transform-object-assign",
        [
            "transform-object-rest-spread",
            {
                "useBuiltIns": true
            }
        ],
        [
            "transform-react-jsx",
            {
                "pragma": "h"
            }
        ]
    ],
    "presets": [
        "env",
        "es2015",
        "stage-2",
        "react"
    ]
}